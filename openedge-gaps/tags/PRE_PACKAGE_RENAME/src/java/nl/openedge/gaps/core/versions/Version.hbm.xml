<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
	<class name="nl.openedge.gaps.core.versions.Version" table="GAPS_V">
		<id
			column="ID"
			name="id"
			type="java.lang.Long"
			unsaved-value="null"
		>
			<generator class="native" />
		</id>
		<property
			name="name"
			column="NAME"
			type="string"
			not-null="true"
			length="40"
		/>
		<property
			column="GELDIG_VANAF"
			name="geldigVanaf"
			not-null="true"
			type="date"
		 />
		<property
			column="GOEDGEKEURD"
			name="goedgekeurd"
			not-null="true"
			type="boolean"
		 />
	</class>
</hibernate-mapping>
