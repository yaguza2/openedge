<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
	<class name="nl.openedge.gaps.core.groups.impl.GroupWrapper"
	table="GAPS_G">
		<id
			column="ID"
			name="id"
			type="java.lang.Long"
			unsaved-value="null"
		>
			<generator class="native" />
		</id>
		<property
			column="PATH"
			name="path"
			type="string"
			length="255"
		 />
		<property
			column="VID"
			name="versionId"
			type="string"
			length="40"
		 />
		<property
			column="DATA"
			name="data"
			type="serializable"
		 />
	</class>
</hibernate-mapping>
