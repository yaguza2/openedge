<?xml version="1.0"?>
<?xml-stylesheet type="text/css" href="../xhtml2.css" ?>
<!DOCTYPE html [
	<!ATTLIST xforms:model id ID #IMPLIED>
	<!ATTLIST xforms:instance id ID #IMPLIED>
	<!ATTLIST section id ID #IMPLIED>
	<!ATTLIST h       id ID #IMPLIED>
	<!ATTLIST div     id ID #IMPLIED>
	<!ATTLIST nl      id ID #IMPLIED>
]>
<html xmlns="http://www.w3.org/2002/06/xhtml2"
    xmlns:xforms="http://www.w3.org/2002/xforms"
    xmlns:ev="http://www.w3.org/2001/xml-events"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xml:lang="en">
    <head>
        <title>Instance referring to an unobtainable external document.</title>
        <xforms:model id="m_001">
            <xforms:instance id="i_001" src="bad.reference"/>
        </xforms:model>
    </head>
    <body>
        <xforms:group>
            <xforms:label>"instance" element refers to an external source which will not be found and should
       generate an xforms-link-exception.</xforms:label>
        </xforms:group>
    </body>
</html>
