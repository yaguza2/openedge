  <build>
    <nagEmailAddress>info@openedge.nl</nagEmailAddress>
    <sourceDirectory>src/java</sourceDirectory>
    <unitTestSourceDirectory>src/test</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
      <resources>
        <resource>
          <directory>${basedir}/src/test</directory>
          <includes>
            <include>**/*.properties</include>
            <include>**/*.xml</include>
            <include>**/*.xsd</include>
            <include>**/*.dtd</include>
            <include>**/*.vm</include>
            <include>**/*.config</include>
            <include>**/*.cfg</include>
            <include>**/*.policy</include>
          </includes>
        </resource>
      </resources>
    </unitTest>
    <resources>
      <resource>
        <directory>${basedir}/src/java</directory>
        <includes>
          <include>**/*.properties</include>
          <include>**/*.xml</include>
          <include>**/*.xsd</include>
          <include>**/*.dtd</include>
          <include>**/*.vm</include>
          <include>**/*.config</include>
        </includes>
      </resource>
    </resources>
  </build>
